{
  "name": "rose-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --port 2999",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start": "npm run serve",
    "unit-test": "jest --testPathPattern=tests/unit/*",
    "func-test": "jest --testPathPattern=tests/func/*",
    "unit-test-ci": "export TEST_ENV=CI && jest --testPathPattern=tests/unit/* --ci --watchAll=false",
    "func-test-ci": "export TEST_ENV=CI && jest --testPathPattern=tests/func/* --ci --watchAll=false"
  },
  "dependencies": {
    "@azure/msal-browser": "^2.2.0",
    "@vueform/toggle": "^2.0.1",
    "core-js": "^3.8.3",
    "dat-gui": "^0.5.0",
    "iv-3d-lib": "^1.0.31",
    "register-service-worker": "^1.7.2",
    "three": "^0.126.1",
    "three-anaglypheffect": "^1.0.0",
    "three-asciieffect": "^1.0.1",
    "three-buffer-geometry-utils": "^1.0.0",
    "three-css3drenderer": "^1.0.0",
    "three-orbitcontrols": "^1.1.2",
    "three-shader-terrain": "^1.0.0",
    "three-shaders": "^1.0.0",
    "three-stereoeffect": "^1.0.15",
    "three-trackballcontrols": "^0.9.0",
    "three.regular.helvetiker": "^1.0.0",
    "tween.js": "^16.6.0",
    "vue": "^2.6.12",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^8.5.1",
    "vue-router": "^3.5.2"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-eslint": "^4.5.13",
    "@vue/cli-plugin-pwa": "^4.5.13",
    "@vue/cli-plugin-router": "^4.5.13",
    "@vue/cli-plugin-typescript": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "chromedriver": "^93.0.1",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-vue": "^6.2.2",
    "jest": "^27.2.0",
    "prettier": "^1.19.1",
    "selenium-webdriver": "^4.0.0-rc-1",
    "ts-jest": "^27.0.5",
    "typescript": "^3.9.10",
    "vue-template-compiler": "^2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/typescript/recommended",
      "@vue/prettier",
      "@vue/prettier/@typescript-eslint"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {
      "no-inferrable-types": "off",
      "@typescript-eslint/no-inferrable-types": "off",
      "prefer-const": "off",
      "no-empty": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
